			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.ИспользоватьРегламентноеЗадание</Field>
				<Field>Список.ФлагИспользования</Field>
				<Field>Список.ФлагИспользованияВыгрузки</Field>
				<Field>Список.ФлагИспользованияЗагрузки</Field>
